acl lans { {%for network in dns_networks%} {{ network }}; {%endfor%} };

options {
        directory "/var/cache/bind";

        forwarders {
            {%for forwarder in dns_forwarders-%} 
            {{ forwarder }};
            {%endfor%}};

        dnssec-validation no;

};


zone "{{ startup_name }}" {
    type master;
    file "/etc/bind/db.clounsible";
};
